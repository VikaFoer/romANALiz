# Air-bot â€“ example env (copy to .env and fill)
# =============================================

# App
APP_ENV=development
LOG_LEVEL=INFO
# PORT=8000   # Railway sets PORT automatically

# Database (use one)
DATABASE_URL=sqlite+aiosqlite:///./data/airbot.db
# DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/airbot

# Kafka (optional)
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=airbot.events
KAFKA_ENABLED=false

# Webhook (optional)
WEBHOOK_URL=
WEBHOOK_ENABLED=false

# SOC-MINT / NLP (optional)
SOC_MINT_ENABLED=false
NLP_MODEL_PATH=

# Worker pool
WORKER_POOL_SIZE=4
QUEUE_MAX_SIZE=10000
